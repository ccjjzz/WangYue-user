<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/common_item_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="13dp"
            android:text="流沙包"
            android:textColor="#ff292621"
            android:textSize="15sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/common_item_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="15dp"
            android:text="2135166317"
            android:textColor="#ff292621"
            android:textSize="12sp"
            app:layout_constraintLeft_toRightOf="@id/common_item_name"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/common_item_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:layout_marginTop="9dp"
            android:text="北京市  北京市  朝阳区"
            android:textColor="#ff292621"
            android:textSize="13sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/common_item_name" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/common_item_addressHouse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="9dp"
            android:text="花园路甲220号花园路 甲25号写字楼230室"
            android:textColor="#ff292621"
            android:textSize="13sp"
            app:layout_constraintLeft_toRightOf="@id/common_item_address"
            app:layout_constraintTop_toBottomOf="@id/common_item_name" />

        <View
            android:id="@+id/common_item_view"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="18dp"
            android:background="@color/colorEee"
            app:layout_constraintTop_toBottomOf="@id/common_item_addressHouse" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/common_item_hook"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="14dp"
            android:src="@drawable/ic_contract_hook"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/common_item_view" />


        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="11dp"
            android:layout_marginTop="14dp"
            android:text="已设为默认"
            android:textColor="#fffd4473"
            android:textSize="12sp"
            app:layout_constraintLeft_toRightOf="@id/common_item_hook"
            app:layout_constraintTop_toBottomOf="@id/common_item_view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/common_item_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="13dp"
            android:text="删除"
            android:textColor="#ff8a857c"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/common_item_view" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/common_item_img"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_marginEnd="5dp"
            android:src="@drawable/ic_common_delete"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/common_item_delete"
            app:layout_constraintTop_toBottomOf="@id/common_item_view" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/common_item_compile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="13dp"
            android:text="编辑"
            android:textColor="#ff8a857c"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/common_item_img"
            app:layout_constraintTop_toBottomOf="@id/common_item_view" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/common_item_img1"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_marginEnd="5dp"
            android:src="@drawable/ic_common_compile"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/common_item_compile"
            app:layout_constraintTop_toBottomOf="@id/common_item_view" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>